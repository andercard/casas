.home-search
    position relative
    z-index 1
    overflow hidden
   
    &:after
        content ''
        position absolute
        background-color white
        bottom -145px
        left -13px
        width 120%
        height 200px
        z-index 2
        transform rotate(3deg)

.home-search-menu
    display flex
    justify-content space-between
    width 100%
    height 100vh
    background-image url(../images/c&h_image_bg_002.jpg)
    background-size cover
    
    > nav 
        display flex
        
        img
            align-self flex-start
            
        ul
            display flex
            list-style none
        
    .home-search-icon
        width 20%
        
        > a
            margin-right 30px
            
        
